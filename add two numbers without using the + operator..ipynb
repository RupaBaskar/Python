   "source": [
    "def no_plus_sum(a,b):\n",
    "\n",
    "    while b != 0:\n",
    "        carry = a & b\n",
    "        a = a ^ b\n",
    "        b = carry << 1\n",
    "    return a\n",
    "no_plus_sum(10,44)"
