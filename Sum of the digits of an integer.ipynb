   "source": [
    "Write a program that will return the sum of the digits of an integer."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter your number: 2349\n",
      "Sum of the digits of an integer is = 18\n"
     ]
    }
   ],
   "source": [
    "Number = int(input(\"Enter your number: \"))\n",
    "sum=0\n",
    "while(Number > 0):\n",
    "    Reminder=Number%10\n",
    "    sum=sum+Reminder\n",
    "    Number=Number//10\n",
    "print(\"Sum of the digits of an integer is = %d\" %sum)\n",
    "    "
